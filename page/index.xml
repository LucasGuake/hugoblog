        <item>
        <title>技术链接 | LIST</title>
        <link>http://localhost:1313/%E6%8A%80%E6%9C%AF%E9%93%BE%E6%8E%A5-list/</link>
        <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
        
        <guid>http://localhost:1313/%E6%8A%80%E6%9C%AF%E9%93%BE%E6%8E%A5-list/</guid>
        <description>&lt;div style=&#34;text-align: center; padding: 2rem;&#34;&gt;
  &lt;!-- 修改 font-size 为更大的值，比如 4rem --&gt;
  &lt;h5 id=&#34;typewriter&#34; style=&#34;font-size: 4rem; line-height: 1.6;&#34;&gt;&lt;/h5&gt;
&lt;/div&gt;
&lt;script&gt;
const text = &#34;在学习的路上，坚持下来的人，都会收获多多。&#34;;
const el = document.getElementById(&#39;typewriter&#39;);
let i = 0;
function typing() {
  if (i &lt;= text.length) {
    el.textContent = text.slice(0, i);
    i++;
    setTimeout(typing, 200);
  }
}
typing();
&lt;/script&gt;</description>
